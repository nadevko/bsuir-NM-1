   "value": "TA[a_,i_]:=If[i==1,0,a[[i,i-1]]]\nTB[a_,i_]:=a[[i,i]]\nTC[a_,i_]:=If[i==Length@a,0,a[[i,i+1]]]\nTD[d_,i_]:=d[[i]]\n\nTL[a_,1]:=TC[a,1]/TB[a,1]\nTL[a_,i_]:=TL[a,b,i]=TC[a,i]/(TB[a,i]-TA[a,i]*TL[a,i-1])\n\nTM[a_,d_,1]:=TD[d,1]/TB[a,1]\nTM[a_,d_,i_]:=TM[a,d,i]=(TD[d,i]-TA[a,i]*TM[a,d,i-1])/(TB[a,i]-TA[a,i]*TL[a,i-1])\n\nTX[a_,d_]:=Table[TX[a,d,i],{i,Length@a}]\nTX[a_,d_,i_]:=TM[a,d,i]-If[\n    i==Length@a,0,\n    TL[a,i]*TX[a,d,i+1]\n]\n\nTable[TL[A,i],{i,Length@A}]\nTable[TM[A,B,i],{i,Length@A}]\nTX[A,B]",
