   "value": "AbsErr[a_,b_,fn_]:=NormMax[First@fn[a,b]-LinearSolve[a,b]]\nRelErr[a_,b_,fn_]:=AbsErr[a,b,fn]/NormMax@LinearSolve[a,b]\nResidual[a_,b_,fn_]:=a*(LinearSolve[a,b]-First@fn[a,b])-b\nNormResidual[a_,b_,fn_]:=NormMax@Residual[a,b,fn]\nMakeRow[a_,b_,fn_,n_]:={\n    \"n=\"<>ToString@n,\n    Seidel[Ad@n,Bd@n]//N//Last,\n    AbsErr[a,b,fn]//N,\n    RelErr[a,b,fn]//N,\n    NormResidual[a,b,fn]//N\n}\n\nPrettyPrinter@Join[\n    {{\n        \"Порядок системы\",\n        \"Количество итераций\",\n        \"Норма вектора абс. погрешности\",\n        \"Норма вектора отн. погрешности\",\n        \"Норма вектора невязки\"\n    },\n    {\"Метод Якоби\"}},\n    MakeRow[Ad@#,Bd@#,Jacobi,#]&/@orders,\n    {{\"Метод Зейделя\"}},\n    MakeRow[Ad@#,Bd@#,Seidel,#]&/@orders\n]",
